{% extends "OCBackBundle::layout.html.twig" %}

{% block content %}
    <section id="content">
        <div class="container">
            <header class="page-header">
                <h3>Tableau de bord<small>Statistiques globales</small></h3>
            </header>

            <div class="overview row">
                <div class="col-md-3 col-sm-6">
                    <div class="o-item bg-cyan">
                        <div class="oi-title">
                            <span data-value="450382"></span>
                            <span>Trafic du site</span>
                        </div>

                        <div class="overview-chart-line text-center"><!-- 9,5,6,3,9,7,5,4,6,5,6,4,9 --></div>
                    </div>
                </div>

                <div class="col-md-3 col-sm-6">
                    <div class="o-item bg-amber">
                        <div class="oi-title">
                            <span data-value="8737"></span>
                            <span>Total des ventes</span>
                        </div>

                        <div class="overview-chart-line text-center"><!-- 2,4,6,5,6,4,5,3,7,3,6,5,9,6 --></div>
                    </div>
                </div>

                <div class="col-md-3 col-sm-6">
                    <div class="o-item bg-green">
                        <div class="oi-title">
                            <span data-value="450382"></span>
